{
  "name": "ongoing/datatrans-bundle",
  "description": "Symfony2 bundle for providing access to Datatrans. This is an extension to the JMSCorePaymentBundle.",
  "type": "symfony-bundle",
  "keywords": [
    "payment",
    "datatrans"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Frederik Trostorf",
      "email": "frederik.trostorf@ongoing.ch"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "jms/payment-core-bundle": "~1.0",
    "academe/omnipay-datatrans": "^3.0",
    "league/omnipay": "^3.0",
    "symfony/http-foundation": "^2.8 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4 || ^5",
    "squizlabs/php_codesniffer": "^3.0@dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Ongoing\\DatatransBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Ongoing\\DatatransBundle\\Tests\\": "/Tests/"
    }
  }
}
